{
    "code-runner.executorMap": {
        "c": "cd $dir && mkdir -p build && gcc-5 $fileName -o build/$fileNameWithoutExt && $dir/build/$fileNameWithoutExt",
        "cpp": "cd $dir && mkdir -p build && gcc-5 $fileName -o build/$fileNameWithoutExt -lstdc++ && $dir/build/$fileNameWithoutExt"
    },
    "cpplint.filters": ["-legal/copyright", "-whitespace/newline"]
}